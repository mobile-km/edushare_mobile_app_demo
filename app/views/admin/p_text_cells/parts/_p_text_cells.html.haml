.p-text-cells
  %ul
    - p_text_cells.each do |p_text_cell|
      %li
        .show
          %span.title= link_to p_text_cell.title, admin_p_text_cell_path(p_text_cell)

          = render "admin/p_text_cells/parts/action", :p_text_cell => p_text_cell

        :ruby
          @child_cells = PTextCell.find(p_text_cell.id).children

        .child
          = render "admin/p_text_cells/parts/child_cells" ,:child_cells  => @child_cells